        ],
        "type": "object",
        "properties": {
          "id": {
            "type": "integer",
            "format": "int32"
          },
          "address": {
            "type": "string",
            "format": "byte",
            "nullable": true
          },
          "hostname": {
            "type": "string",
            "nullable": true
          },
          "subnetID": {
            "type": "integer",
            "format": "int32"
          },
          "monitorStateList": {
