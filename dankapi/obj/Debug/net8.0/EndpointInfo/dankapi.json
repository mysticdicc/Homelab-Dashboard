  "components": {
    "schemas": {
      "DashboardItem": {
        "required": [
          "displayName",
          "id",
          "url"
        ],
        "type": "object",
        "properties": {
          "id": {
            "type": "integer",
            "format": "int32"
          },
          "displayName": {
            "type": "string",
            "nullable": true
          },
          "url": {
            "type": "string",
