            },
            "nullable": true
          },
          "isMonitoredICMP": {
            "type": "boolean"
          },
          "isMonitoredTCP": {
            "type": "boolean"
          },
          "portsMonitored": {
            "type": "array",
            "items": {
              "type": "integer",
              "format": "int32"
            },
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "MonitorState": {
        "required": [
          "submitTime"
        ],
        "type": "object",
        "properties": {
          "id": {
            "type": "integer",
            "format": "int32"
          },
          "iP_ID": {
            "type": "integer",
            "format": "int32"
          },
          "submitTime": {
            "type": "string",
            "format": "date-time"
          },
          "icmpResponse": {
            "type": "boolean",
            "nullable": true
          },
          "portState": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/PortState"
            },
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "PortState": {
        "required": [
          "port",
          "status"
        ],
        "type": "object",
        "properties": {
          "id": {
            "type": "integer",
            "format": "int32"
          },
          "monitorID": {
            "type": "integer",
            "format": "int32"
          },
          "monitorState": {
            "$ref": "#/components/schemas/MonitorState"
          },
          "port": {
            "type": "integer",
            "format": "int32"
          },
          "status": {
            "type": "boolean"
          }
        },
        "additionalProperties": false
      }
